<SettingsFlyout x:Uid="Settings" x:Class="TsinghuaNet.Settings" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="using:TsinghuaNet" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" IconSource="Assets/SmallLogo.png" d:DesignWidth="346" HorizontalContentAlignment="Stretch" Unloaded="SettingsFlyout_Unloaded" Loaded="SettingsFlyout_Loaded">
    <StackPanel Style="{StaticResource SettingsFlyoutSectionStyle}" VerticalAlignment="Top">
        <TextBlock x:Uid="Theme" Style="{StaticResource TitleTextBlockStyle}" />
        <ComboBox x:Name="comboBoxTheme" SelectionChanged="comboBoxTheme_SelectionChanged" Margin="0,10">
            <ComboBoxItem x:Uid="ThemeDark" />
            <ComboBoxItem x:Uid="ThemeLight" />
        </ComboBox>
    </StackPanel>
</SettingsFlyout>
